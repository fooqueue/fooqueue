{
  "name": "fooqueue",
  "version": "0.1.25",
  "type": "module",
  "description": "Job queue for serverless Typescript apps",
  "source": "src/index.ts",
  "bin": "cli.js",
  "exports": "./lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf ./lib && npm run build:server && parcel build",
    "build:client": "parcel build",
    "watch": "parcel watch",
    "build:server": "tsc",
    "dev": "parcel watch",
    "lint": "eslint . --ext .ts",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build:server",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "check-updates": "npx npm-check-updates",
    "check-updates:minor": "npx npm-check-updates --target minor",
    "check-updates:patch": "npx npm-check-updates --target patch",
    "start": "node lib/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fooqueue/fooqueue.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "typescript",
    "npm",
    "queue",
    "redis",
    "serverless",
    "fooqueue",
    "ts"
  ],
  "author": "Django Merope Synge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fooqueue/fooqueue"
  },
  "homepage": "https://github.com/fooqueue/fooqueue#readme",
  "devDependencies": {
    "@parcel/packager-ts": "^2.10.3",
    "@parcel/transformer-typescript-types": "^2.10.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "ansi-regex": ">=6.0.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.1",
    "parcel": "^2.10.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "bullmq": "^4.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fooqueue": "^0.1.0",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "yargs": "^17.7.2"
  }
}
