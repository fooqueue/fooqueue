{
  "name": "fooqueue",
  "version": "0.1.0",
  "description": "Job queue for serverless Typescript apps",
  "main": "lib/client/index.js",
  "bin": "cli.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "check-updates": "npx npm-check-updates",
    "check-updates:minor": "npx npm-check-updates --target minor",
    "check-updates:patch": "npx npm-check-updates --target patch",
    "start": "node lib/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fooqueue/fooqueue.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "typescript",
    "npm",
    "queue",
    "redis",
    "serverless",
    "fooqueue",
    "ts"
  ],
  "author": "Django Merope Synge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fooqueue/fooqueue"
  },
  "homepage": "https://github.com/fooqueue/fooqueue#readme",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "ansi-regex": ">=6.0.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "bullmq": "^4.13.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "yargs": "^17.7.2"
  }
}
